main = {
    let a = allocDArray:[Ref Int32] 10; in
      print_i64 (prim_arrayLength a);
      arrayFizz a;
      expect_i64 (prim_arrayLength a);
    end
};

arrayFizz = { forall t:Type, a : Array t =>
  print_i64 (prim_arrayLength a);
  force_gc_for_debugging_purposes !;
  expect_i64 (prim_arrayLength a);
};

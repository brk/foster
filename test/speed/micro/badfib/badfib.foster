// IN: 40

badfib = fn (x:i32) { if x < 3 then { 1 } else { badfib(x - 1) + badfib(x - 2) } }

timefn = fn (f:fn() to i64) {
  let s:i64 = llvm.readcyclecounter() in {
    f()
    let e:i64 = llvm.readcyclecounter() in {
      (e - s)
    } : i64
  } : i64
}

main = fn () {
  let n:i32 = read_i32() in {
    print_i64(timefn( fn () { print_i32(badfib(n)) } ))
  }
}

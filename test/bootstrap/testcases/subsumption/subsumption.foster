main = {
  test-okay !;
  test-notokay !;
  ()
};

// To check an expression E against a term T,
// we can write { v : { T => () }   =>   v E }

test-okay = {
  expect_i1 True;
  print_i1 (__COMPILES__ { v : {     { forall x:Boxed, { x => x } => () }    => () } =>
                           v                    { f : { () => () } => () }   });
};


test-notokay = {
  expect_i1 False;
  print_i1 (__COMPILES__ { v : {     { { () => () } => () }    => () } =>
                v { f : forall x:Boxed, { x => x } => () } });
};


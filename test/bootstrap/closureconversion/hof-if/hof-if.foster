prim-and = { a : i1 => b: i1 =>
  if a then b else false end
};

hof-and = { a : {i1} => b: {i1} =>
  if a() then b() else false end
};

main = {
  let x = opaquely_i32 2;
      y = opaquely_i32 3;
      z = opaquely_i32 4;
  in
    expect_i1 true;  print_i1 (prim-and (x < y) (y < z));
    expect_i1 false; print_i1 (prim-and (x < y) (z < y));
    expect_i1 true ; print_i1 (hof-and { x < y } { y < z });
  end
};

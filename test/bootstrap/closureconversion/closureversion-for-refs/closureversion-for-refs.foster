main = {
//  test-clo-in-rawfn !;
  test-rawfn-in-clo !;
};

test-rawfn-in-clo = {
  let n    = 5;
      clo  = { x:Int32 => x +Int32 n };
      cell = (ref clo);
   in
      expect_i32 7;
      print_i32 (cell^ 2);

      inc >^ cell;

      expect_i32 9;
      print_i32 (cell^ 8);
  end
};

inc = { x : Int32 => x +Int32 1; };

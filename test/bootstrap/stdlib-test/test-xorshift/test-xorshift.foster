snafuinclude Prelude  "prelude";
snafuinclude Xorshift "prng/xorshift";
snafuinclude KnuthLCG "prng/knuthLCG";

print_point = { i => x =>
  print_i64_bare (zext_i32_to_i64 i);
  print_text_bare ": ";
  print_i64x (zext_i32_to_i64 x);
};

main = {

  st = xor1x32MakeState 2463534242;
  expect_i32x 0x2B1F4D63;
  expect_i32x 0x94DACB7A;
  foldRange 0 2 st { i => st =>
    case xor1x32gen st of (x, st) ->
      print_i32x x;
      st
    end
  };


  gen = kLCG1x64HOF 2223;

  expect_i64 8081521364565807322;
  print_i64 (gen !);

  expect_i64 1898400345152178178;
  print_i64 (gen !);
};


snafuinclude List "list";

main = {
  expect_i32 5;
  print_i32 (listLength (mkUnitListTo 5));

  expect_i32 0;
  print_i32 (listLength (Nil:[()] !));
};

mkUnitListTo = { n : Int32 =>
  if n ==Int32 0 then
    Nil:[()] !
  else
    Cons () (mkUnitListTo (n -Int32 1))
  end
};

mkPairListTo = { n : Int32 =>
  if n ==Int32 0 then
    Nil:[(Int32, Int32)] !
  else
    Cons (n, n) (mkPairListTo (n -Int32 1))
  end
};


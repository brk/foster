// Need separate function because LLVM structs must be indexed by const val
print_0_of_t_i32_i1 = { x : (i32, i1) => print_i32 x[0] };
print_1_of_t_i32_i1 = { x : (i32, i1) => print_i1  x[1] };

main = {
  expect_i32 42;
  print_0_of_t_i32_i1 (42, true);

  expect_i1 true;
  print_1_of_t_i32_i1 (42, true);
  0
};

snafuinclude Text "text";

main = {
  expect_text "abc";
  print_text  'abc';

  expect_text "abc";
  print_text  """abc""";

  expect_text "abc";
  print_text  '''abc''';

  expect_text "abc";
  print_text r'abc';

  expect_text "abc";
  print_text r"abc";

  expect_text "abc";
  print_text r"""abc""";

  expect_text "abc";
  print_text r'''abc''';

  expect_text "a\\nc";
  print_text r"a\nc";

  expect_text """a\\bc""";
  print_text r"""a\bc""";

  expect_text "a\nb";
  print_text """a
b""";

  test-byte-literals !;
};

test-byte-literals = {
  expect_text "ab c";
  print-bytes b"ab\x20c";

  expect_text "abcd";
  print-bytes b"""ab
cd""";
};

print-bytes = { a : Array Int8 =>
  prim_print_bytes_stdout a (arrayLength32 a); print_newline !; ()
};

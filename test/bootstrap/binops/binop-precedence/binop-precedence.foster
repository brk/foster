fn "main" () {
  expect_i32(14) ;  print_i32(2 + 3 * 4)
  expect_i32(10) ;  print_i32(2 * 3 + 4)
  
  expect_i32((10 - 5) - 3) ; print_i32(10 - 5 - 3)
  expect_i32((10 - 5) + 3) ; print_i32(10 - 5 + 3)
  
  expect_i32(2) ; print_i32(3 - 5 + 7 - 9 - 11 + 17)
  
  expect_i32(4) ; print_i32(        7 - 9 - 11 + 17)

  expect_i1(true); print_i1(2 * 3   <=   5 + 1)
  expect_i1(true); print_i1(2 * 3   ==   5 + 1)
  expect_i1(true); print_i1(2 < 3   ==   4 <= 5)

  expect_i32(25) ;  print_i32(2 + 3 * 4 + 5 + 6)
  expect_i32(68) ;  print_i32(2 + 3 * 4 * 5 + 6)
  expect_i32(32) ;  print_i32(2 * 3 + 4 * 5 + 6)
  expect_i32(39) ;  print_i32(7 * 1 + 2 * 3 + 4 * 5 + 6)
  
  expect_i32(3)  ;  print_i32(20 - 10 - 5 - 2)
  
  expect_i32(1205)  ; print_i32(1+3+(5+7)*9*11+13)
  expect_i32(1201)  ; print_i32((5+7)*9*11+13)
  
  expect_i32(508)  ; print_i32((5)*9*11+13)
  
  expect_i32(311)  ; print_i32(1+3*9*11+13)
  expect_i32(126);  print_i32(2 * 3 + 4 * 5 * 6)
 
}

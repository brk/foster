
main = {
  expect_i32 1111111;
  expect_i32 2222222;
  expect_i32 4444444;
  expect_i32 10;
  expect_i32 6666;
  expect_i32 1234;
  expect_i32 20;
  expect_i32 5555555;
  expect_i32 6666666;
  expect_i32 7777;
  expect_i32 3333333;
  expect_i32 9999999;

  print_i32 1111111;
  co = coro_create { x : Int32 =>
         print_i32 4444444;
         force_gc_for_debugging_purposes  !;
         print_i32 x; // 10
         y = coro_yield:[Int32,Int32] 6666;

         force_gc_for_debugging_purposes !;
         coro_yield:[Int32,Int32] y;
         print_i32 5555555;
         force_gc_for_debugging_purposes !;
         print_i32 6666666;
         // This should be a type error but it isn't yet.
         coro_yield:[Int32,{Int32}] { 7777 };
         9999
  };
  r  = (prim ref 0);

    print_i32 2222222;
  force_gc_for_debugging_purposes !;
    print_i32 (coro_invoke co 10); // 6666
    print_i32 1234;
  force_gc_for_debugging_purposes !;
    print_i32 (coro_invoke co 20); // 20
  force_gc_for_debugging_purposes !;
    print_i32 (coro_invoke co 30);
    print_i32 3333333;
    print_i32 9999999;
};



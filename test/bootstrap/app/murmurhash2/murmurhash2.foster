mrmr-step2 = fn (km:i32, r:i32) { bitxor(km, bitashr(km, r)) }
mrmr-step5 = fn (h:i32, kx:i32, m:i32) { bitxor(h * m, kx * m)  }
murmurhash2-step-manually-lifted = fn (k:i32, h:i32, r:i32, m:i32) {
  mrmr-step5(h, mrmr-step2(k * m, r), m)
}

murmurhash2-step-let-transformed = fn (k:i32, h:i32, r:i32, m:i32 to i32) {
  fn    (km:i32) { // step 2
    fn  (kx:i32) { // step 5
      bitxor(h * m, kx * m)
    } (bitxor(km, bitashr(km, r)))
  }   (k * m)
}

murmurhash2-step-let = fn (k:i32, h:i32, r:i32, m:i32 to i32) {
  let km:i32 = k * m
  let kx:i32 = bitxor(km, bitashr(km, r)) in {
    bitxor(h * m, kx * m)
  }
}

